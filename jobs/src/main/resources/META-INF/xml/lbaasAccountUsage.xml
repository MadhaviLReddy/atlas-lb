<productSchema xmlns="http://docs.rackspace.com/core/usage/schema"
               namespace="http://docs.rackspace.com/usage/lbaas/account"
               serviceCode="CloudLoadBalancers"
               version="1"
               groupByResource="false"
               resourceTypes="TENANT">
    <description>
        Lbaas load balancer tenant  usage fields.
    </description>
    <attribute name="numLoadbalancers" type="int" use="required"
               unitOfMeasure="COUNT" groupBy="true">
        The number of load balancers associated with the tenant.
    </attribute>
    <attribute name="numPublicVips" type="int" use="required"
               unitOfMeasure="COUNT" groupBy="true">
        The number of public virtual ips associated with the tenant.
        record.
    </attribute>
    <attribute name="numServicenetVips" type="int" use="required"
               unitOfMeasure="COUNT" groupBy="true">
        The amount of ssl bandwidth in, in bytes.
    </attribute>
</productSchema>
