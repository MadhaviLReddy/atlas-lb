<productSchema xmlns="http://docs.rackspace.com/core/usage/schema"
               namespace="http://docs.rackspace.com/usage/lbaas"
               serviceCode="CloudLoadBalancersUsage"
               version="1"
               resourceTypes="LOADBALANCING">
    <description>
        Lbaas load balancer usage fields.
    </description>
    <attribute name="avgConcurrentConnections" type="double" use="required"
               unitOfMeasure="int" groupBy="true">
        The amount of conncurrent connections in bytes.
    </attribute>
    <attribute name="avgConcurrentConnectionsSsl" type="double" use="required"
               unitOfMeasure="int" groupBy="true">
        The amount of conncurrent ssl connections in bytes.
    </attribute>
    <attribute name="bandWidthIn" type="long" use="required"
               unitOfMeasure="B" groupBy="true">
        The amount of bandwidth in, in bytes.
    </attribute>
    <attribute name="bandWidthOut" type="long" use="required"
               unitOfMeasure="B" groupBy="true">
        The amount of bandwidth out in bytes.
    </attribute>
    <attribute name="bandWidthInSsl" type="long" use="required"
               unitOfMeasure="B" groupBy="true">
        The amount of ssl bandwidth in, in bytes.
    </attribute>
    <attribute name="bandWidthOutSsl" type="long" use="required"
               unitOfMeasure="B" groupBy="true">
        The amount of ssl bandwidth out in bytes.
    </attribute>
    <attribute name="numPolls" type="integer" use="required"
               unitOfMeasure="int" groupBy="true">
        The number of polls per record.
    </attribute>
    <attribute name="numVips" type="integer" use="required"
               unitOfMeasure="int" groupBy="true">
        The number of vips per record.
    </attribute>
    <attribute name="tagsBitmask" type="integer" use="required"
               unitOfMeasure="int" groupBy="true">
        the BitMask tags.
    </attribute>
    <attribute name="eventType" type="string" use="required"
                groupBy="true">
        The event type.
    </attribute>
    <attribute name="accountId" type="integer" use="required"
               groupBy="true">
        The account id.
    </attribute>
</productSchema>