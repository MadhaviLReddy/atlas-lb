<?xml version="1.0" encoding="UTF-8"?>

<content-normalization xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
   xmlns='http://docs.api.rackspacecloud.com/repose/content-normalization/v1.0'
   xsi:schemaLocation='http://docs.api.rackspacecloud.com/repose/content-normalization/v1.0'>

   <header-filters>
       <blacklist id="restricted-headers"/>
    </header-filters>

    <media-types>
        <media-type name="application/xml" variant-extension="xml" />
        <media-type name="application/atom+xml" variant-extension="atom" />
        <media-type name="application/json" variant-extension="json" />
        <media-type name="*/*" variant-extension="wildcard" preferred="true" />
    </media-types>
</content-normalization>
